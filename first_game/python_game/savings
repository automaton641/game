# convert into JSON:
data_json = json.dumps(data, indent=4)
wins_a = 0
wins_b = 0
draws = 0
processed = []
average = 0.0
average_list = []
read_data = load_data("data.json")
for game in read_data:
    turns = game["turns"]
    #print("appending", turns)
    processed.append(turns)
    average += turns
n = len(processed)
average /= n
for i in range(n):
    average_list.append(average)
plt.plot(processed, 'ro', average_list, 'b-')
plt.show()